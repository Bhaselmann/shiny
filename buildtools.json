{
    "$schema": "https://mobilebuildtools.com/schemas/v2/buildtools.schema.json",
    "projectSecrets": {
        "Shiny.Tests.Device": {
            "delimiter": ";",
            "disable": false,
            "namespace": "Secrets",
            "className": "Values",
            "properties": [
                {
                    "name": "AppCenterKey",
                    "type": "String"
                },
                {
                    "name": "FirebaseServerApiKey",
                    "type": "String"
                },
                {
                    "name": "AzureNotificationHubListenerConnectionString",
                    "type": "String"
                },
                {
                    "name": "AzureNotificationHubFullConnectionString",
                    "type": "String"
                },
                {
                    "name": "AzureNotificationHubName",
                    "type": "String"
                }
            ]
        },
        "Samples": {
            "delimiter": ";",
            "disable": false,
            "namespace": "Secrets",
            "className": "Values",
            "properties": [
                {
                    "name": "AppCenterKey",
                    "type": "String",
                    "defaultValue": null
                },
                {
                    "name": "AzureNotificationHubListenerConnectionString",
                    "type": "String",
                    "defaultValue": null
                },
                {
                    "name": "AzureNotificationHubName",
                    "type": "String",
                    "defaultValue": null
                }
            ]
        }
    }
}