<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:platform="clr-namespace:Sample.Platform"
             x:DataType="platform:ConnectivityViewModel"
             x:Class="Sample.Platform.ConnectivityPage"
             Title="Connectivity">

    <TableView>
        <TableRoot>
            <TableSection>
                <TextCell Text="Access" Detail="{Binding Access}" />
                <TextCell Text="Connection Via" Detail="{Binding Types}" />
            </TableSection>
        </TableRoot>
    </TableView>
</ContentPage>
